{
  "name": "@exsui/ui",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "src",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:dev": "webpack --mode=development",
    "build:css": "tailwindcss -i ./styles.css -o ./dist/index.css",
    "build:webpack": "webpack --mode=production --node-env=production",
    "dev": "webpack serve --mode=development",
    "docs": "typedoc --out docs ./src/index.ts",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@exsui/utils": "workspace:*",
    "@floating-ui/react": "^0.26.10",
    "@headlessui/react": "^1.7.18",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.22",
    "react-icons": "^5.0.1",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@exsui/eslint-config": "workspace:*",
    "@exsui/typescript-config": "workspace:*",
    "@microsoft/api-extractor": "^7.43.0",
    "@swc/core": "^1.4.11",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.22",
    "@types/react-transition-group": "^4.4.10",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@webpack-cli/generators": "^3.0.7",
    "autoprefixer": "^10.4.19",
    "css-loader": "^6.10.0",
    "deepmerge": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "4.0.0-next.54",
    "typedoc-plugin-mdn-links": "^3.1.18",
    "typedoc-plugin-missing-exports": "^2.2.0",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "workbox-webpack-plugin": "^7.0.0"
  }
}
